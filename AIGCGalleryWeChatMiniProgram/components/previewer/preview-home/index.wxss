.preview-cnt {
  position: absolute;
  width: 100vw;
  height: 100vh;
  left: 0;
  top: 0;
  z-index: 5;
}

.preview-list {
  width: 100vw;
  height: 100vh;
  background-color: #000;
}

.need-hide-on-back {
  position: absolute;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
}

.share-element-image {
  position: absolute;
  width: 100vw;
  height: 100vh;
  top: 0;
  left: 0;
  background-color: transparent;
}

.share-element-image .temp-image {
  width: 100%;
  height: 100%;
  background-color: transparent;
}

.preview-video {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: transparent;
}

.share-element-image .temp-image image {
  width: 100%;
  height: 100%;
}

.preview-middle-self {
  position: absolute;
  width: 100vw;
  height: 100vh;
  background-color: #000;
  z-index: 0;
}